{
  "require": {
    "craftcms/cms": "^4.4.0",
    "vlucas/phpdotenv": "^5.4.0",
    "craftcms/redactor": "3.0.4",
    "craftsnippets/craft-image-toolbox": "2.2.3",
    "mmikkel/cp-field-inspect": "1.4.4",
    "nystudio107/craft-fastcgicachebust": "4.0.0",
    "oofbar/craft-twig-toolbox": "1.0.3",
    "putyourlightson/craft-blitz-recommendations": "2.1.3",
    "putyourlightson/craft-elements-panel": "2.0.0",
    "putyourlightson/craft-sprig": "2.7.2",
    "sebastianlenz/linkfield": "2.1.5",
    "spicyweb/craft-neo": "3.9.10",
    "spicyweb/craft-quick-field": "2.0.7",
    "studioespresso/craft-dumper": "3.0.1",
    "verbb/buttonbox": "4.2.0",
    "verbb/field-manager": "3.0.8",
    "verbb/super-table": "3.0.12"
  },
  "require-dev": {
    "craftcms/generator": "^1.3.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  }
}
